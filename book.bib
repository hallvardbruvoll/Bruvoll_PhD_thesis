  author = {{M{\"u}ller-Schee{\ss}el}, Nils and Huke{\v l}ov{\'a}, Zuzana and Meadows, John and Cheben, Ivan and M{\"u}ller, Johannes and Furholt, Martin},
  year = {2021},
